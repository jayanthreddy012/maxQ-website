<% include partials/header.ejs %>
<%let date;%>>
<table>
    <tr>
        <th>SHIPMENT_ID</th>
        <th>EVENT_TIME</th>
        <th>BOX_ID</th>
        <th>TEMP1</th>
        <th>TEMP2</th>
        <th>BATT</th>
    </tr>
    <% data.forEach(function (box){ %>
    <tr>
        <td><%= box.shipment_id %></td>
        <td><%= box.event_time %></td>
        <% if(box.data != undefined){ %>
        <td><%= box.box_id %></td>
        <td><%= box.data.temp1 %></td>
        <td><%= box.data.temp2 %></td>
        <td><%= box.data.batt %></td>
        <% } %>
    </tr>
    <% }) %>
</table>

<% include partials/footer.ejs %>
